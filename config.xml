<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
		xmlns:android   = "http://schemas.android.com/apk/res/android"
        id          = "com.phonegap.webfrankly"
        version     = "1.0.0" >
		
    <!-- versionCode is optional and Android only -->

    <name>WorkoutToGo</name>
	
    <description>
        Workouts to go generate you a S-M-A-R-T workout in secounds where you can save and use for later. 
    </description>

 <author href="https://webfrankly.com" email="frankmckechnie@gmail.com">
        Frank Mckechnie
 </author>
 <access origin="*" />
<preference name="android-minSdkVersion" value="14" />
<gap:platform name="android" />
<preference name="fullscreen" value="false" />
<preference name="android-windowSoftI nputMode" value="stateVisible" />

<plugin name="cordova-plugin-file" spec="~4.3.0" />
<plugin name="cordova-plugin-device" spec="~1.1.3" />
<plugin name="cordova-plugin-zip" />
<plugin name="cordova-plugin-file-transfer" spec="~1.6.1" />
<plugin name="cordova-plugin-media-extended" />

<uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
<preference name="AndroidPersistentFileLocation" value="Compatibility" />
<preference name="AndroidExtraFilesystems" value="files,files-external,documents,sdcard,cache,cache-external,assets,root" />
<!-- Default application icon -->
<icon src="img/logo.png" />


</widget>


